{
  "name": "simple-express-api-crud",
  "version": "1.0.0",
  "description": "simple api crud with middleware",
  "main": "app.js",
  "scripts": {
    "test": "jest"
  },
  "author": "Muhammad Alfalah Madukubah",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "jest": "^27.0.5"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/controller/**/*.js"
    ]
  }
}
